{
  "name": "strategy-twitter",
  "version": "0.1.0",
  "description": "Sammler.io strategy for twitter.",
  "license": "MIT",
  "author": "Stefan Walther",
  "main": "index.js",
  "repository": "git@github.com:sammler/strategy-twitter.git",
  "scripts": {
    "d-build": "docker build -t sammler/auth-service .",
    "dc-dev-down": "docker-compose --f=./docker-compose.dev.yml down",
    "dc-up:dev": "docker-compose --f=./docker-compose.dev.yml up",
    "dc-dev-up-d": "docker-compose --f=./docker-compose.dev.yml up -d",
    "start": "node index.js",
    "start:dev": "nodemon index.js",
    "test": "npm run test:integration",
    "test:watch": "nodemon --exec npm run test:integration",
    "test:integration": "mocha './test/integration/**/*.spec.js' --require './test/mocha.conf.js' --timeout 6000",
    "lint:src": "eslint src",
    "lint:src:fix": "eslint src --fix"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "convict": "^4.0.0",
    "express": "^4.15.4",
    "express-result": "^0.1.1",
    "find-pkg": "^1.0.0",
    "http-status-codes": "^1.2.0",
    "increase-memory-limit": "^1.0.3",
    "js-yaml": "^3.9.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mongoose": "^4.11.10",
    "mongoose-connection-promise": "^0.2.2",
    "mongoose-timestamp": "^0.6.0",
    "sammler-nodelib-logger": "^0.2.1",
    "swagger-ui-express": "^2.0.7",
    "twit": "^2.2.9",
    "winster": "^0.2.5"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.6.1",
    "eslint-config-space-single": "^0.2.0",
    "mocha": "^3.5.0",
    "supertest": "^3.0.0"
  },
  "engines": {
    "node": ">7.6.0"
  },
  "verb": {
    "run": true,
    "toc": false,
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": []
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "verb",
      "verb-readme-generator"
    ]
  }
}
