{
  "name": "strategy-twitter",
  "version": "0.1.0",
  "description": "Sammler.io strategy for twitter.",
  "license": "MIT",
  "author": "Stefan Walther",
  "main": "index.js",
  "repository": "git@github.com:sammler/strategy-twitter.git",
  "scripts": {
    "dc-down:dev": "docker-compose --f=./docker-compose.dev.yml down",
    "dc-up:dev": "docker-compose --f=./docker-compose.dev.yml up --build",
    "docs": "docker run --rm -v ${PWD}:/opt/verb stefanwalther/verb",
    "precommit": "npm run docs"
  },
  "dependencies": {
  },
  "devDependencies": {
  },
  "engines": {
    "node": ">8.4.0"
  },
  "verb": {
    "run": true,
    "toc": false,
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": []
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "verb",
      "verb-readme-generator"
    ]
  }
}
